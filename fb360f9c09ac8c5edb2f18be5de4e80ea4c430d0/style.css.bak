.breathing /*cool breathing animation, no use yet*/
{
animation:10s breather linear infinite;
-webkit-animation:10s breather linear infinite;
-moz-animation:10s breather linear infinite;
}

@-moz-keyframes breather {  
 0% { color: black; }
 25% { color: white; }
 50% { color: black; }
 75% {color: white; }
 100% { color: black; }
 }

@-webkit-keyframes breather {  
 0% { color: black; }
 25% { color: white; }
 50% { color: black; }
 75% {color: white; }
 100% { color: black; }
 }

@keyframes breather {  
 0% { color: black; }
 25% { color: white; }
 50% { color: black; }
 75% {color: white; }
 100% { color: black; }
 }

