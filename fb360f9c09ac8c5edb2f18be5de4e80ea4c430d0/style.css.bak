#blogimage
{
position: static;
margin: auto;
width: 100%; 
transition: -webkit-transform 0.5s ease;
}

#blogimage:hover
{
-webkit-transform: scale(2.5);
z-index: 1;
}

#enlarge 
{
width:100%;
transition: -webkit-transform 0.5s ease;
cursor:zoom-in;
}

#enlarge:active 
{
-webkit-transform: scale(7);
cursor:zoom-out;
}


